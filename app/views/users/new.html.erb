<h1>Registration </h1>


<%= form_for :user, url: "/users" do |f| %>
<p>  FirstName: <br />  <%= f.text_field :firstname%></p>
<p>  LastName: <br />  <%= f.text_field :lastname%></p>
<p>  Email:<br /> <%= f.text_field :email %></p>
<p>  Password: <br />  <%= f.text_field :password %></p>
<p>  Password-confirmation: <br />  <%= f.text_field :password_confirmation%></p>
<%= f.submit %>
<% end %>

